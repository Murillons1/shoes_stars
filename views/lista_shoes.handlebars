<h1>Lista dos Sneakers Cadastrados</h1>
<br>
<table>
    <thead>
        <th>Nome</th>
        <th>Qtde Estoque</th>
        <th>Preço R$</th>
        <th>Descrição</th>
    </thead>
    <tbody>
    {{#each valores}}
        <tr>
            <td>{{this.nome}}</td>
            <td>{{this.quantidadeEstoque}}</td>
            <td>{{this.precoUnitario}}</td>
            <td>{{this.descricao}}</td>
        </tr>
    {{/each}} 
    </tbody>
</table>
